{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "fs:allow-read",
    "fs:allow-remove",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "**/*"
        }
      ]
    },
    "allow-archive-commands",
    "allow-db-commands",
    "allow-splash-commands",
    "opener:default"
  ]
}